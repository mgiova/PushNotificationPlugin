      <group targetFramework="uap">
        <dependency id="Newtonsoft.Json" version="11.0.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!--NetStandard/PCL-->
    <file src="..\src\Plugin.PushNotification\bin\Release\netstandard2.0\Plugin.PushNotification.*" target="lib\netstandard2.0" />
    <file src="..\src\Plugin.PushNotification.Android\bin\Release\Plugin.PushNotification.*" target="lib\MonoAndroid81" />
    <file src="..\src\Plugin.PushNotification.iOS\bin\iPhone\Release\Plugin.PushNotification.*" target="lib\Xamarin.iOS10" />
    <file src="..\src\Plugin.PushNotification.UWP\bin\Release\Plugin.PushNotification.*" target="lib\uap" />
  </files>
</package>
